                }
            });

        }

        function GetExchangeRateByCurrencyCode(currencyCode) {
            LoadingAnimation();
            var def = $.Deferred();
            $.ajax({
                url: '@Url.Content("~/PayRoll/GetExRateByCurrencyCode?currencyCode=")' + currencyCode,
                method: 'GET',
                contentType: 'application/json',
                success: function (data) {
                    StopLoadingAnimation();
                    def.resolve(data);
                }
            });
            return def.promise();
        }

