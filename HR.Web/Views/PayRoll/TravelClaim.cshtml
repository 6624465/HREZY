                if (isNaN(amount)) {
                    amount = 0;
                }
                var exchangeRate = parseFloat($('#claimDetailAirfareVm_' + index + '__ExchangeRate').val());
                if (isNaN(exchangeRate)) {
                    exchangeRate = 1;
